@model RTEXERP.Contracts.BLModels.Maxco.ViewModel.RemoveOrderCostingViewModel
<div class="row">
    <div class="form-group">
        <label asp-for="CostingID" class="col-sm-4 control-label label-ajt-sm required"></label>
        <div class="col-sm-8">
            <select asp-for="CostingID" multiple asp-items="Model.DDLOrderNo" class="form-control form-control-sm"></select>
            <span asp-validation-for="CostingID" class="text-danger"></span>
        </div>
    </div>

    <div class="form-group">
        <label asp-for="Remarks" class="col-sm-4 control-label label-ajt-sm required"></label>
        <div class="col-sm-8">
            <input asp-for="Remarks" class="form-control form-control-sm" autocomplete="off" />
            <span asp-validation-for="Remarks" class="text-danger"></span>
        </div>
    </div>
</div>


@section scripts{
    <script type="text/javascript">
        $(function () {

            $("#ChequeID").chosen();



        })
    </script>
    
}